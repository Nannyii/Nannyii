//#define定义的标识符常量
  #define MAX 100
    //#define 可以定义宏-带参数
int main()
{
  int a =MAX;//100
  return 0;
 
}
 //#define 可以定义宏-带参数
//函数的实现方式
int MAX(int x,int y)
{
  if(x>y)
    return x;
 else
   return y;
}
//宏的实现方式
#define MAX(X,Y) (X>Y?X:Y)
int main()
{
  int a=10；
   int b=20；
    //函数方式写法
    int max=MAX(a,b);
  printf("max=%d\n",max);
  //宏方式写法
  max=MAX(a,b);
  printf("max=%d\n",max);
  return 0;
  }
