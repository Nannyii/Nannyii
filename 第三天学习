#include<stdio.h>
int main()
{
  //计算两个数的和
  int num1=0;
  int num2=0;
  int sum=0;
  //输入数据-使用输入函数scanf
  scanf("%d%d",&num1,&num2);//&取地址符号-定位到内存中的特定位置，像学号一样   
    sum= num1 + num2;
    printf("%d\n",sum);
    return 0;
}
