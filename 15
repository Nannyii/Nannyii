int main()
{
  int age =20;
  if(age<18)
    printf("未成年%d\n");//什么都不输出
return 0;
}
int main()
{
  int age =20;
  if(age<18)
    printf("未成年%d\n");
  else
    printf("成年%d\n");
return 0;
}
int main()
{
  int age =20;
  if(age<18)
    printf("未成年\n");
  else if(age>=18 && age<28)
    printf("青年\n");
  else if(age>=28 && age<50)
    printf("壮年\n");
  else if(age>=50 && age<90)
    printf("老年\n");
else
    printf("长寿\n");
return 0;
}
